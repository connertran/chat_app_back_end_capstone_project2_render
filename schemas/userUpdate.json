{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "password": {
      "type": "string"
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "gmailAddress": {
      "type": "string",
      "minLength": 6,
      "maxLength": 70,
      "format": "email"
    },
    "bio": {
      "type": "string"
    }
  },
  "required": ["password", "firstName", "lastName", "gmailAddress", "bio"]
}
